# Server Configuration
SERVER_PORT=8080
SERVER_WORKER_PROCESSES=auto
SERVER_WORKER_CONNECTIONS=1024
SERVER_KEEPALIVE=75

# System Configuration
SYSTEM_CLEANUP_INTERVAL=10

# Logging Configuration
LOGGING_LEVEL=notice
LOGGING_BUFFER_SIZE=4k
LOGGING_FLUSH_INTERVAL=1s
LOGGING_ACCESS_LOG=true
LOGGING_ERROR_LOG=true

# Security Configuration
SECURITY_RATE_LIMIT_REQUESTS=100
SECURITY_RATE_LIMIT_WINDOW=60
SECURITY_RATE_LIMIT_BURST=120
SECURITY_IP_BAN_MAX_FAILS=10
SECURITY_IP_BAN_DURATION=3600
SECURITY_SSL_ENABLED=true
SECURITY_SSL_PROTOCOLS="TLSv1.2 TLSv1.3"

# Cache Configuration
CACHE_ENABLED=true
CACHE_DEFAULT_TTL=3600
CACHE_MAX_SIZE=10m
CACHE_MIN_USES=2

# Proxy Configuration
PROXY_READ_TIMEOUT=60
PROXY_SEND_TIMEOUT=60
PROXY_CONNECT_TIMEOUT=60
PROXY_BUFFER_SIZE=4k
PROXY_BUFFERS="8 4k"

# Monitoring Configuration
MONITORING_ENABLED=true
MONITORING_METRICS_ENABLED=true
MONITORING_HEALTH_CHECK_INTERVAL=5
MONITORING_HEALTH_CHECK_TIMEOUT=3

# CORS Configuration
CORS_ENABLED=true
CORS_ALLOW_PROTOCOLS=https,http
CORS_ALLOW_ORIGINS=http://example.com,https://example.com
CORS_ALLOW_METHODS=GET,POST,PUT,DELETE,OPTIONS
CORS_ALLOW_HEADERS=Content-Type,Authorization,X-Request-ID
CORS_EXPOSE_HEADERS=X-Request-ID
CORS_MAX_AGE=3600
CORS_ALLOW_CREDENTIALS=true

# CORS Validation Configuration
CORS_VALIDATION_MAX_ORIGIN_LENGTH=2048
CORS_VALIDATION_MAX_SUBDOMAIN_COUNT=10
CORS_VALIDATION_MAX_SUBDOMAIN_LENGTH=63

# CORS Common Headers Configuration
CORS_COMMON_HEADERS="host,accept,accept-encoding,accept-language,content-type,content-length,origin,access-control-request-method,access-control-request-headers"